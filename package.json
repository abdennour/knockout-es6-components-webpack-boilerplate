{
  "name": "knockout-es6-components-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate Knockout , ES6 classes components driven and webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --config webpack/config.js --open",
    "build": "webpack --mode production --config webpack/config.js",
    "help": "Install nvm, and switch to Node 8.5.0 : nvm install 8.5.0",
    "test": "mocha-webpack --require src/setupTests.js --webpack-config webpack/config.test.js --recursive \"src/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Abdennour TOUMI <in.abdennoor.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "css-loader": "^0.28.11",
    "expect": "^22.4.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jsdom": "^11.7.0",
    "jsdom-global": "^3.0.2",
    "mini-css-extract-plugin": "^0.4.0",
    "mocha": "^5.0.5",
    "mocha-webpack": "^2.0.0-beta.0",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  },
  "dependencies": {
    "knockout": "^3.4.2"
  }
}
